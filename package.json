{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/fragment-matcher": "^3.3.3",
    "@graphql-codegen/introspection": "^2.2.3",
    "@storybook/addon-a11y": "6.5.14",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-docs": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/addons": "6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/eslint-config-storybook": "^3.1.2",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/history": "^4.7.11",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "bootstrap": "^5.2.3",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "29.3.1",
    "jest-cli": "^29.3.1",
    "jest-resolve": "29.3.1",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.9.4",
    "validate-branch-name": "^1.3.0"
  },
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/schema-ast": "^2.6.0",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-operations": "^2.5.10",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-tools/mock": "^8.7.15",
    "@graphql-tools/schema": "^9.0.13",
    "@sentry/react": "^7.24.2",
    "@sentry/tracing": "^7.24.2",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "buffer": "^6.0.3",
    "clsx": "^1.2.1",
    "css-loader": "^6.7.2",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "husky": "^8.0.2",
    "i18next": "^22.4.1",
    "jwt-decode": "^3.1.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^12.1.1",
    "react-router-dom": "^6.4.5",
    "react-toastify": "^9.1.1",
    "sass": "^1.56.2",
    "stylelint-config-standard-scss": "^6.1.0",
    "vite": "^4.1.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 9009 --no-manager-cache",
    "prepare": "husky install",
    "generate:gql": "graphql-codegen",
    "generate:gql:watch": "graphql-codegen --watch",
    "stylint": "npx stylelint \"**/*.scss\"",
    "lint:css": "stylelint '**/*.{scss,ts,tsx}'",
    "lint:css:fix": "stylelint --fix '**/*.scss'",
    "lint:js": "eslint --ext .js,.ts,.tsx,.graphql .",
    "lint:js:fix": "npm run lint:js -- --fix .",
    "lint:prettier": "prettier -c .",
    "lint:prettier:fix": "prettier -w ."
  },
  "jest": {
    "moduleNameMapper": {
      "/^sentry\\/(.*)$/": "<rootDir>/src/sentry/$1",
      "/^exceptions\\/(.*)$/": "<rootDir>/src/exceptions/$1",
      "/^services\\/(.*)$/": "<rootDir>/src/services/$1",
      "/^gql\\/(.*)$/": "<rootDir>/src/gql/$1",
      "/^modules\\/(.*)$/": "<rootDir>/src/modules/$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "validate-branch-name && npm test",
      "post-merge": "git diff-tree -r --name-only --no-commit-id ORIG_HEAD HEAD | grep --quiet package-lock.json && npm ci"
    }
  },
  "lint-staged": {
    "*!(.d).(ts|tsx)": [
      "eslint --fix"
    ],
    "*.(ts|tsx|graphql|css)": [
      "prettier --write  --ignore-path .eslintignore"
    ]
  },
  "validate-branch-name": {
    "pattern": "^master$|stable$|^v\\d+.\\d+$|^release/v\\d+.\\d+$|^(fix|chore|poc|feature|style)[_][\\w-]+$|^IM-[0-9]+[\\w-]+$",
    "errorMsg": "Branch name format accepted: IM-XXX_some_keyworks, vXX.XX or (fix|chore|poc)_some_keyworks"
  }
}
